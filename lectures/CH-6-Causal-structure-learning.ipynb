    "def plot_from_model(model):\n",
    "    # plot\n",
    "    g = gr.Digraph()\n",
    "    \n",
    "    for i in range(0, len(model_estimated['model_edges'])):\n",
    "        g.edge(*model_estimated['model_edges'][i])\n",
    "    return g\n",
    "\n",
    "# Plot the DAG\n",
    "plot_from_model(model_estimated)"
